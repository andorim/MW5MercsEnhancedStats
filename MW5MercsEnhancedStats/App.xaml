<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MW5MercsEnhancedStats"
             xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2" x:Class="MW5MercsEnhancedStats.App"
             StartupUri="MainWindow.xaml"
             Startup="Application_Startup"
             Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Buttons.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Label.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Textbox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Combobox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/ComboBoxItem.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/ListBox.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
